        <script>
            
            // Method 1 to initialize the array
            var arr = new Array();
            arr[0]="javascript";
            arr[1]=50;
            //console.log(arr)  // to print the output in the console tab
            
            //Method 2 to initialize the array
            var tweets = ["hello",10,"world"];
            //console.log

            tweets.push(200); // inserts new element at the end of the array

            tweets.pop(); // delete the last element from the array
        </script>
